{
  "name": "single-spa-portal-example-frame",
  "version": "1.0.0",
  "description": "Portal app that builds the foundation of multiple SPAs",
  "scripts": {
    "start": "webpack-dev-server --port 9000",
    "start:app0": "cd ../app0React/ && npm run watch:portal",
    "start:app1": "cd ../app1React/ && npm run watch:portal",
    "start:app2": "cd ../app2Angular/ && npm run watch:portal",
    "start:app3": "cd ../app3Angular1/ && npm run watch:portal",
    "start:app4": "cd ../app4vue/ && npm run watch:portal",
    "start:app5": "cd ../app5Angular/ && npm run watch:portal",

    "inst": "npm i",
    "inst:app0": "cd ../app0React/ && npm i",
    "inst:app1": "cd ../app1React/ && npm i",
    "inst:app2": "cd ../app2Angular/ && npm i",
    "inst:app3": "cd ../app3Angular1/ && npm i",
    "inst:app4": "cd ../app4vue/ && npm i",
    "inst:app5": "cd ../app5Angular/ && npm i",
    "build": "webpack -p --progress"
  },
  "author": "Michael Ruecker",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.5"
  },
  "dependencies": {
    "copy-webpack-plugin": "^4.5.1",
    "single-spa": "^4.0.0",
    "systemjs": "^0.21.4",
    "zone.js": "^0.8.26"
  }
}
